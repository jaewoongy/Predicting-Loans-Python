      "cell_type": "code",
      "source": [
        "def loan(x):\n",
        "    if x == 'Fully Paid':\n",
        "        x = 1\n",
        "    if x == 'Charged Off':\n",
        "        x = 0\n",
        "    return x"
      ],
      "metadata": {
        "id": "L1kT50Hj3E2v"
      },
      "execution_count": null,
      "outputs": []
