        "model.add(Dropout(0.5))\n",
        "model.add(Dense(43, activation = 'relu'))\n",
        "model.add(Dropout(0.5))\n",
        "model.add(Dense(21, activation = 'relu'))\n",
        "model.add(Dropout(0.5))\n",
        "model.add(Dense(1, activation = 'sigmoid'))\n",
        "\n",
        "model.compile(optimizer = 'adam', loss = 'binary_crossentropy')"
